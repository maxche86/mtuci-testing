{
  "name": "skeleton-vite",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite --host --port 5188 --open",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode dev",
    "build:test": "tsc && vite build --mode test",
    "build:prod": "tsc && vite build --mode prod",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "clean": "rd /s /q dist",
    "lint-has-warnings": "eslint \"**/*.ts[x]\" --max-warnings=0",
    "lint-all": "eslint --fix .",
    "lint-cache": "eslint --cache --fix .",
    "ts:check": "tsc --skipLibCheck --noEmit",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxche86/mtuci-testing.git"
  },
  "dependencies": {
    "@mantine/core": "^7.14.0",
    "@mantine/dates": "^7.14.0",
    "@mantine/hooks": "^7.14.0",
    "@mantine/vanilla-extract": "^7.14.0",
    "@tanstack/react-query": "^5.61.5",
    "@vanilla-extract/css": "^1.16.0",
    "classnames": "^2.5.1",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "zustand": "^5.0.7",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@tanstack/eslint-plugin-query": "^5.61.4",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vanilla-extract/vite-plugin": "^4.0.17",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-svgr": "^4.3.0",
    "nodemon": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint-has-warnings",
      "eslint --cache --fix",
      "prettier -w -u"
    ]
  }
}
